# Wallabag RSS Tool Configuration
# Copy this file to .env and update with your values

# Wallabag instance URL
WALLABAG_BASE_URL=https://your-wallabag-instance.com

# Wallabag OAuth2 client credentials
# You can find these in your Wallabag settings under "API clients management"
WALLABAG_CLIENT_ID=your_client_id_here
WALLABAG_CLIENT_SECRET=your_client_secret_here

# Wallabag user credentials
WALLABAG_USERNAME=your_username
WALLABAG_PASSWORD=your_password